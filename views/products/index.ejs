<%- include('../partials/_head') %>

  <main>
    <h1>All Products</h1>
  
    <% products.forEach((product => { %>
     <section>
        <h4>
          <a href="/products/<%= product._id %>">
            <img src="<%= product.image %>" alt="image" width="200px">
          </a>
        </h4>
     </section>
    <% })) %>
  </main>

<%- include('../partials/_footer') %>
